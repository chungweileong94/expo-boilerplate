{
	"expo": {
		"name": "expo-playground",
		"slug": "expo-playground",
		"version": "1.0.0",
		"scheme": "expo-playground",
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/favicon.png"
		},
		"newArchEnabled": true,
		"plugins": [
			[
				"expo-router",
				{
					"root": "./src/app"
				}
			],
			"expo-web-browser"
		],
		"experiments": {
			"typedRoutes": true,
			"tsconfigPaths": true,
			"reactCompiler": true
		},
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.anonymous.expo-playground"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		}
	}
}
